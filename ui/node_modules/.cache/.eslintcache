   '& .MuiTextField-root': {\n        margin: theme.spacing(1),\n        width: '300px',\n        },\n        '& .MuiButtonBase-root': {\n        margin: theme.spacing(2),\n        },\n    },\n}));\n\nexport default function Login(props) {\n    const { user: currentUser } = useSelector((state) => state.auth);\n    if(currentUser) {\n      return <Navigate to=\"/\"/>\n    }\n    const form = useRef();\n    const classes = useStyles();\n    const dispatch = useDispatch();\n\n    // create state variables\n    const [email, setEmail] = useState('');\n    const [password, setPassword] = useState('');\n    const [loading, setLoading] = useState(false);\n    const { isLoggedIn } = useSelector(state => state.auth);\n    const { message } = useSelector(state => state.message);\n\n    const handleSubmit = e => {\n        e.preventDefault();\n        setLoading(true);\n        dispatch(login(email, password))\n            .then(() => {\n                props.history.push(\"/\");\n                window.location.reload();\n            }).catch(() => {\n                setLoading(false);\n            }\n        );\n    };\n    \n    if (isLoggedIn) {\n        return <Navigate to=\"/\" />;\n    }\n  \n    return(\n        <div className='flex column flex-center'>\n            <form \n                ref={form}\n                className={classes.root}\n                onSubmit={handleSubmit}>\n                <h2>User login</h2>\n\n                {message && (\n                    <div className=\"alert-danger\" role=\"alert\">\n                        <span>{message}</span>\n                    </div>\n                )}\n\n                <TextField \n                    label=\"Email\"\n                    variant=\"filled\"\n                    type=\"email\"\n                    required\n                    value={email}\n                    onChange={e => setEmail(e.target.value)}\n                />\n                <TextField\n                    label=\"Password\"\n                    variant=\"filled\"\n                    type=\"password\"\n                    value={password}\n                    onChange={e => setPassword(e.target.value)}\n                    required\n                />\n\n                <div className='btn-wrapper'> \n                    <Button type=\"submit\" variant=\"contained\" color=\"primary\">\n                        login\n                    </Button>\n                </div>\n            </form>\n            {loading && (\n                <div className=\"flex flex-center spinner-wrapper\">\n                    <span>loading ...</span>\n                </div>\n            )}\n        </div>\n    )\n}","/home/projects/pern-app/pern-ui/src/components/404/notfound.js",[],[],"/home/projects/pern-app/pern-ui/src/components/student/student.js",["91","92","93"],[],"/home/projects/pern-app/pern-ui/src/components/subjects/subjects.js",[],[],"/home/projects/pern-app/pern-ui/src/components/dashboard/dashboard.js",[],[],"/home/projects/pern-app/pern-ui/src/actions/types.js",[],[],"/home/projects/pern-app/pern-ui/src/services/auth.service.js",["94"],[],{"ruleId":"95","severity":2,"message":"96","line":33,"column":18,"nodeType":"97","endLine":33,"endColumn":24},{"ruleId":"95","severity":2,"message":"98","line":34,"column":21,"nodeType":"97","endLine":34,"endColumn":30},{"ruleId":"95","severity":2,"message":"99","line":35,"column":22,"nodeType":"97","endLine":35,"endColumn":33},{"ruleId":"95","severity":2,"message":"100","line":38,"column":31,"nodeType":"97","endLine":38,"endColumn":39},{"ruleId":"95","severity":2,"message":"100","line":39,"column":37,"nodeType":"97","endLine":39,"endColumn":45},{"ruleId":"95","severity":2,"message":"100","line":40,"column":35,"nodeType":"97","endLine":40,"endColumn":43},{"ruleId":"95","severity":2,"message":"101","line":41,"column":28,"nodeType":"97","endLine":41,"endColumn":39},{"ruleId":"95","severity":2,"message":"101","line":42,"column":25,"nodeType":"97","endLine":42,"e